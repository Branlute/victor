<UserControl x:Class="VictorNamespace.PopupSplash"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="800" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" Background="{StaticResource PhoneChromeBrush}" Width="480" Height="800">
        <ProgressBar Height="46" HorizontalAlignment="Left" Margin="12,232,0,202" Name="progressBar1" Width="456" IsIndeterminate="True" VerticalAlignment="Center" />
        <TextBlock Height="37" HorizontalAlignment="Left" Margin="12,349,0,0" Name="textBlock1" Text="Loading" VerticalAlignment="Top" Width="456" TextAlignment="Center" />
    </Grid>
</UserControl>
